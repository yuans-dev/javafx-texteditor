                        <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="DOWN" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TextArea fx:id="textArea" prefHeight="235.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
